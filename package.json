{
  "name": "tick-tock",
  "version": "0.1.0",
  "description": "React & SASS playground to play around with designs of time keeping apps",
  "watch": true,
  "main": "index.js",
  "scripts": {
    "start": "npm run start-dev",
    "start-dev": "npx webpack-dev-server --config=./configs/webpack/dev.js",
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "start-prod": "npm run build && node server.js",
    "clean-dist": "rm -rf -d dist",
    "test": "jest --watch --coverage --config=./configs/jest.conf",
    "lint": "./node_modules/.bin/eslint ./src",
    "eject": "rm -rf .git && rm README.md && echo \"#SOME_PROJECT\" > README.md"
  },
